{
    "name": "sloveniangooner/locale-anywhere",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Sloveniangooner\\LocaleAnywhere\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sloveniangooner\\LocaleAnywhere\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
